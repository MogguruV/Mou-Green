<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".EdukasiMenu">

    <ImageButton
        android:id="@+id/edukasiToMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="57dp"
        android:background="@drawable/edukasi_panah_kembali" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="55dp"
        android:layout_height="19dp"
        android:layout_toEndOf="@id/edukasiToMenu"
        android:layout_alignTop="@id/edukasiToMenu"
        android:layout_marginStart="16dp"
        app:srcCompat="@drawable/edukasi_teks_edukasi" />

    <ImageButton
        android:id="@+id/edukasiKonten1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="120dp"
        android:background="@drawable/edukasi_berita_1" />

    <ImageButton
        android:id="@+id/edukasiKonten2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/edukasiKonten1"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="21dp"
        android:background="@drawable/edukasi_berita_2" />

</RelativeLayout>
